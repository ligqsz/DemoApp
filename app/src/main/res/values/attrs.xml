<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleView">
        <attr name="circle_color" format="color" />
        <attr name="text_color" format="color" />
        <attr name="text_size" format="dimension" />
        <attr name="text_str" format="string" />
    </declare-styleable>

    <declare-styleable name="TickView">
        <attr name="ring_width" format="dimension" />
        <attr name="ring_radius" format="dimension" />
        <attr name="checked_color" format="color" />
        <attr name="unchecked_color" format="color" />
        <attr name="checked_tick_color" format="color" />
        <attr name="background_color" format="color" />
    </declare-styleable>
</resources>